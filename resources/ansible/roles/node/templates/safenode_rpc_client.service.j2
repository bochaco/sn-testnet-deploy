[Unit]
Description=Safe node RPC client Service

[Service]
ExecStart={{ node_archive_dest_path }}/safenode_rpc_client \
  {{ node_rpc_ip }}:{{ node_rpc_port }} \
  transfers {{ node_cash_notes_dir_path }} \
  5f15ae2ea589007e1474e049bbc32904d583265f12ce1f8153f955076a9af49b
StandardOutput=journal
StandardError=inherit
User=safe

[Install]
WantedBy=multi-user.target
